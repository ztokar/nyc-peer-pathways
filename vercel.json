{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.rise2growth.com" }],
      "destination": "https://rise2growth.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/((?!sitemap\\.xml|robots\\.txt|favicon\\.ico|og-image\\.png|placeholder\\.svg).*)", "destination": "/index.html" }
  ]
}
